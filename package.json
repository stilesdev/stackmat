{
    "name": "stackmat",
    "version": "1.1.1",
    "description": "Subscribe to events received from a Stackmat timer connected to the browser via the HTML5 Audio API.",
    "keywords": [
        "Stackmat",
        "Rubik's Cube"
    ],
    "homepage": "https://github.com/stilesdev/stackmat",
    "bugs": "https://github.com/stilesdev/stackmat/issues",
    "license": "MIT",
    "author": {
        "name": "Matthew Stiles",
        "url": "https://github.com/stilesdev"
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "main": "./dist/stackmat.umd.cjs",
    "module": "./dist/stackmat.js",
    "types": "./dist/stackmat.d.ts",
    "exports": {
        ".": {
            "import": "./dist/stackmat.js",
            "require": "./dist/stackmat.umd.cjs"
        }
    },
    "unpkg": "dist/stackmat.umd.cjs",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint ./src",
        "release": "bumpp package.json"
    },
    "packageManager": "pnpm@8.10.5",
    "devDependencies": {
        "@stilesdev/eslint-config-compat": "^0.6.1",
        "@stilesdev/eslint-config-typescript": "^0.6.1",
        "@types/node": "^18.18.9",
        "bumpp": "^9.2.0",
        "eslint": "^8.53.0",
        "typescript": "^5.2.2",
        "vite": "^5.0.0",
        "vite-plugin-dts": "^3.6.3"
    }
}
